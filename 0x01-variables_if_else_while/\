#include <stdio.h>

/**
 * main - Prints all possible combinations for single digit numbers
 *
 * Return: Always 0 (Success)
 */
char a = 0;
int i = 0;
int main(void)
{
    for (; i < 10 ; i++)
    {
        putchar(i);

        for(; a < 10 ; a++)
        {
           putchar(a);
        }
    }
    putchar(',');
    putchar('\n');
    return (0);
}
